<%#=render plain: params[:usersearch]%>

<h1>Results</h1>
<%@result.each do |x|%>

  <div class="col-md-4">
    <%=image_tag x.artwork %><br>
    <div id="somename"><%=x.name%></div>
    <div id="songartist"><%=x.artist%></div>
    <%=link_to "Add this Song to my Collection", 
                songs_path(:name => x.name, :artist => x.artist, :artwork => x.artwork), 
                                                            method: :post%>
  </div>

<%end%>




